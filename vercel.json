{
	"version": 2,
	"functions": {
		"api/index.py": {
			"runtime": "python3.11",
			"maxDuration": 10,
			"memory": 1024
		}
	},
	"builds": [
		{
			"src": "api/index.py",
			"use": "@vercel/python"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/api/index.py"
		}
	],
	"env": {
		"SECRET_KEY": "@SECRET_KEY",
		"ALGORITHM": "@ALGORITHM",
		"ACCESS_TOKEN_EXPIRE_MINUTES": "@ACCESS_TOKEN_EXPIRE_MINUTES"
	}
}
